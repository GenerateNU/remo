# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Book {
  id: ID!
  title: String!
  author: String!
  user: User!
}

type User {
  id: ID!
  first_name: String!
  last_name: String!
  email: String!
}

type Query {
  books: [Book!]!
}

input NewBook {
  title: String!
  author: String!
  userId: String!
}

type Mutation {
  createBook(input: NewBook!): Book!
}
